{
    "dashboard": {
        "uid": "soroban-db",
        "title": "Soroban Registry â€” Database",
        "timezone": "utc",
        "refresh": "10s",
        "panels": [
            {
                "title": "Query Duration P95",
                "type": "timeseries",
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 0,
                    "y": 0
                },
                "targets": [
                    {
                        "expr": "histogram_quantile(0.95, sum(rate(soroban_db_query_duration_seconds_bucket[5m])) by (le, query))",
                        "legendFormat": "{{query}}"
                    }
                ]
            },
            {
                "title": "Active / Idle Connections",
                "type": "timeseries",
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 12,
                    "y": 0
                },
                "targets": [
                    {
                        "expr": "soroban_db_connections_active",
                        "legendFormat": "active"
                    },
                    {
                        "expr": "soroban_db_connections_idle",
                        "legendFormat": "idle"
                    }
                ]
            },
            {
                "title": "Query Errors",
                "type": "stat",
                "gridPos": {
                    "h": 6,
                    "w": 6,
                    "x": 0,
                    "y": 8
                },
                "targets": [
                    {
                        "expr": "rate(soroban_db_query_errors_total[5m])",
                        "legendFormat": "errors/s"
                    }
                ]
            },
            {
                "title": "Transactions / sec",
                "type": "stat",
                "gridPos": {
                    "h": 6,
                    "w": 6,
                    "x": 6,
                    "y": 8
                },
                "targets": [
                    {
                        "expr": "rate(soroban_db_transactions_total[5m])",
                        "legendFormat": "txn/s"
                    }
                ]
            },
            {
                "title": "Pool Size",
                "type": "gauge",
                "gridPos": {
                    "h": 6,
                    "w": 6,
                    "x": 12,
                    "y": 8
                },
                "targets": [
                    {
                        "expr": "soroban_db_pool_size",
                        "legendFormat": "pool"
                    }
                ]
            }
        ]
    }
}